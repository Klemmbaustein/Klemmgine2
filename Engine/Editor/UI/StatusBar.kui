element StatusBarElement
{
	var Icon = "file:Engine/Editor/Assets/Info.png";
	var Text = "";

	width = 1pn;
	height = 24px;

	verticalAlign = centered;

	child UIBackground
	{
		size = 16px;
		image = Icon;
		leftPadding = 8px;
		rightPadding = 8px;
		color = Color_Text;
	}
	child UIText
	{
		size = 12px;
		text = Text;
		color = Color_Text;
	}
}