element MenuBarButton
{
	var Name = "File";
	leftPadding = 4px;

	child UIButton button
	{
		minSize = (24, 20)px;
		color = Color_LightBackground;
		verticalAlign = centered;

		corners = Theme_CornerSize;

		child UIText
		{
			size = 12px;
			color = Color_Text;
			padding = 4px;

			text = Name;
		}
	}
}